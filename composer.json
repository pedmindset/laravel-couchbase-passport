{
    "name": "designmynight/laravel-couchbase-passport",
    "description": "A package to allow laravel/passport use with pedmindset/laravel-couchbase",
    "homepage": "https://github.com/pedmindset/laravel-couchbase-passport",
    "license": "MIT",
    "keywords": [
        "laravel",
        "laravel-couchbase",
        "laravel-couchbase-passport",
        "laravel-passport",
        "couchbase",
        "passport",
        "pedmindset"
    ],
    "require": {
        "illuminate/support": "^5.5",
        "pedmindset/couchbase": "dev-master",
        "laravel/passport": "4.0.* || 5.0.* || 6.0.* || 7.0.*"
    },
    "autoload": {
        "psr-4": {
            "Pedmindset\\Couchbase\\": "src"
        }
    },
    "authors": [
        {
            "name": "Emmnauel Oduro",
            "email": "emmarthurson@gmail.com",
            "role": "Developer"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Pedmindset\\Couchbase\\CouchbasePassportServiceProvider"
            ]
        }
    }
}
